<?xml version="1.0" encoding="utf-8" ?>
<views:BasePage xmlns="http://xamarin.com/schemas/2014/forms"
             x:DataType="vm:registerViewModel"
             x:TypeArguments="vm:registerViewModel"
             xmlns:views="clr-namespace:MedikTapp.Views.Base"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:vm="clr-namespace:MedikTapp.ViewModels"
             x:Class="MedikTapp.Views.registerViewPage">
    <ContentPage.Content>
        <CollectionView ItemsSource="{Binding _Registers}">
            <CollectionView.ItemTemplate>
                <DataTemplate x:DataType="vm:registerModels">
                    <StackLayout>
                        <Label Text="{Binding AppTitle}"
                               VerticalOptions="CenterAndExpand" 
                               HorizontalOptions="CenterAndExpand" />
                    </StackLayout>
                </DataTemplate>
            </CollectionView.ItemTemplate>
        </CollectionView>
       
    </ContentPage.Content>
</views:BasePage>